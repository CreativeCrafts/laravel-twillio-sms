{
    "name": "creativecrafts/laravel-twillio-sms",
    "description": "A simple package to send sms messages using twillio",
    "keywords": [
        "CreativeCrafts",
        "laravel",
        "laravel-twillio-sms",
        "laravel-send-sms"
    ],
    "homepage": "https://github.com/creativecrafts/laravel-twillio-sms",
    "license": "MIT",
    "authors": [
        {
            "name": "Godspower Oduose",
            "email": "oduose4eva@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3|^8.2",
       "illuminate/contracts": "^12.0|^11.0|^10.0",
        "twilio/sdk": "^7.16",
        "spatie/laravel-package-tools": "^1.19"
    },
    "require-dev": {
        "larastan/larastan": "^2.9|^3.1",
        "laravel/pint": "^1.21",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^7.10|^8.6",
        "orchestra/testbench": "^10.0|^9.11|^8.0",
        "pestphp/pest": "^3.7|^2.35.1",
        "pestphp/pest-plugin-arch": "^3.0|^2.7",
        "pestphp/pest-plugin-laravel": "^3.0|^2.4",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan-phpunit": "^2.0|^1.4",
        "rector/rector": "^2.0|^1.2",
        "symplify/easy-coding-standard": "^12.5.8"
    },
    "autoload": {
        "psr-4": {
            "CreativeCrafts\\LaravelTwillioSms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CreativeCrafts\\LaravelTwillioSms\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-laravel-openid-connect --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CreativeCrafts\\LaravelTwillioSms\\LaravelTwillioSmsServiceProvider"
            ],
            "aliases": {
                "LaravelTwillioSms": "CreativeCrafts\\LaravelTwillioSms\\Facades\\LaravelTwillioSms"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
